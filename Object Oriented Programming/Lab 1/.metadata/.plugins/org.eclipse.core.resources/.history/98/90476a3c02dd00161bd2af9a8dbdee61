package lab1a;

public class Puppy {
	private String name;
	private int age;

	public Puppy() {
		name = "Name not given yet";
		age = 0;
	}

	public Puppy(String name, int age) {
		this.name = name;
		this.age = age;
	}

	public static void main(String[] args) {
		Puppy myPuppy = new Puppy("Waffles", 2);
		System.out.println("Puppy name: " + myPuppy.name);
	}
}
